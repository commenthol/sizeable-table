{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    },
    "prefer-for-of": false,
    "only-arrow-functions": false
  }
}
